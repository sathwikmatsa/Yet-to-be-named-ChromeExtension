{
    "manifest_version": 2,
    "name": "test",
    "version": "0.1",

    "browser_action": {
        "default_title": "timer",
        "default_popup": "popup/popup.html"
    },

    "background": {
        "scripts": [
            "libraries/p5.min.js",
            "libraries/p5.dom.min.js",
            "libraries/p5.sound.min.js",
            "background/background.js"
        ]
    },

    "permissions": [
        "storage",
        "notifications"
    ],
   
    "web_accessible_resources": [
        "assets/ProductSans/*.tff"
    ] 
}
    
