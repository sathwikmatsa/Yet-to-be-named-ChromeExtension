{
    "manifest_version": 2,
    "name": "test",
    "version": "0.1",

    "browser_action": {
        "default_title": "timer",
        "default_popup": "popup/popup.html"
    },

    "background": {
        "scripts": [
            "libraries/p5.min.js",
            "libraries/p5.dom.min.js",
            "libraries/p5.sound.min.js",
            "background/background.js"
        ]
    },

    "permissions": [
        "downloads",
        "notifications",
        "storage"
    ],
   
    "web_accessible_resources": [
        "assets/ProductSans/*.tff"
    ],

    "chrome_url_overrides": {
        "newtab": "newtab/newtab.html"
    }
}
    
